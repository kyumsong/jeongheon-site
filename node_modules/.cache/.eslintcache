nge', (e) => {\n      setThemeName('light') // Ignore system preference, always light\n    });\n  }, [])\n\n  const toggleTheme = () => {\n    const name = themeName === 'dark' ? 'light' : 'dark'\n    localStorage.setItem('themeName', name)\n    setThemeName(name)\n  }\n\n  return (\n    <ThemeContext.Provider value={[{ themeName, toggleTheme }]}>\n      {children}\n    </ThemeContext.Provider>\n  )\n}\n\nThemeProvider.propTypes = {\n  children: PropTypes.node.isRequired,\n}\n\nexport { ThemeProvider, ThemeContext }","/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Header/Header.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/About/About.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Contact/Contact.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Skills/Skills.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Footer/Footer.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Projects/Projects.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/ScrollToTop/ScrollToTop.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/portfolio.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Navbar/Navbar.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/ProjectContainer/ProjectContainer.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Posts/Posts.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/components/Sermons/Sermons.js",[],"/Users/davidsong/Documents/Workspace/cleanfolio/src/pages/PostsPage.js",[],"/Users/davidsong/Documents/Workspace/jeongheon-site/src/index.js",[],"/Users/davidsong/Documents/Workspace/jeongheon-site/src/App.js",[],"/Users/davidsong/Documents/Workspace/jeongheon-site/src/contexts/theme.js",[],"/Users/davidsong/Documents/Workspace/jeongheon-site/src/pages/PostsPage.js",[],"/Users/davidsong/Documents/Workspace/jeongheon-site/src/componen